   </j:member>
   <j:member name="">
      <j:string>empty string</j:string>
   </j:member>
   <j:member name="id">
      <j:string>1234</j:string>
   </j:member>
   <j:member name="'single quotes'">
      <j:string>squoted key</j:string>
   </j:member>
   <j:member name="1foo$bar">
